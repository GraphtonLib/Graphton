{
  "name": "@graphtonlib/graphton",
  "description": "Generate a JS/TS query builder library from an (external) GraphQL introspection schema.",
  "version": "0.6.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/GraphtonLib/Graphton.git"
  },
  "packageManager": "yarn@3.1.1",
  "author": "Robbin \"Roboroads\" Schepers <robbin@deschepers.nl>",
  "license": "MIT",
  "homepage": "https://github.com/GraphtonLib/Graphton",
  "bugs": {
    "url": "https://github.com/GraphtonLib/Graphton/issues"
  },
  "type": "module",
  "bin": "src/graphton.js",
  "keywords": [
    "graphql",
    "typescript",
    "generator"
  ],
  "files": [
    "src/**/*",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "FUNDING.yml"
  ],
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "generate": "yarn generate:ts; yarn generate:js; yarn lint:fix --quiet example",
    "generate:ts": "node src/graphton.js generate ./example/schema.json -o ./example/graphtonGeneratedTypescript.ts",
    "generate:js": "node src/graphton.js generate ./example/schema.json -o ./example/graphtonGeneratedJavascript.js",
    "dev": "tsc --watch",
    "prod": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/eslint": "^8",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.3.0",
    "standard-version": "^9.3.2"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "change-case": "^4.1.2",
    "commander": "^8.3.0",
    "typescript": "^4.5.2"
  }
}
